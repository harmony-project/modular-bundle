{
  "name": "harmony-project/modular-bundle",
  "type": "library",
  "description": "Integrates the Harmony Modular Routing component into the full-stack Symfony framework",
  "keywords": ["routing", "database", "symfony"],
  "homepage": "http://harmony-project.io",
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Goudriaan",
      "homepage": "http://codedmonkey.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "harmony-project/modular-routing": "^0.1",
    "psr/container": "^1.0",
    "symfony/config": "^3.4|^4.0",
    "symfony/dependency-injection": "^3.4|^4.0",
    "symfony/event-dispatcher": "^3.4|^4.0",
    "symfony/framework-bundle": "^3.4|^4.0",
    "symfony/http-foundation": "^3.4|^4.0",
    "symfony/http-kernel": "^3.4|^4.0",
    "symfony-cmf/routing-bundle": "^2.0"
  },
  "require-dev": {
    "doctrine/common": "^2.5",
    "doctrine/orm": "^2.5",
    "sensio/framework-extra-bundle": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Harmony\\Bundle\\ModularBundle\\": "source/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
